
<h1>Users Database</h1>
<a href="/qr">Read QR</a>


<table class="table">
  <thead>
    <tr>
      <th scope="col">Email</th>
      <th scope="col">Name</th>
      <th scope="col">Codigo</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each Users}}
    <tr>
    <th scope="row">{{this.email}}</th>
      <td>{{this.name}}</td>
      <td>{{this.codigo}}</td>
      <td>{{this.status}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>